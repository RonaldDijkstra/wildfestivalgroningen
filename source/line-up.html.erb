---
title: "Line-up"
description: "Be sure to check this page regularly for line-up updates!"
pageable: true
---
<header class="normal-page-header">
  <div class="grid-row">
    <h1>
      <%= dato.line_up_page.title %>
    </h1>
  </div>
</header>

<section class="participants wide-row">
  <div class="grid-row">
    <div class="participants-grid-wrap">
      <div class="participants-grid feed">
        <div class="row">
          <% dato.line_up_page.line_up.each do |record| %>
            <div class="participant-wrap excerpt-box-wrap col">
              <div class="participant excerpt-box">
                <div class="image-wrap">
                  <%= image_tag record.logo.url(fm: 'png', auto: 'compress', class: "image", alt: record.name) %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
  <% if dato.line_up_page.text_below_participant %>
    <div class="grid-row">
      <div class="tba">
        <%= markdownify(dato.line_up_page.text_below_participant) %>
      </div>
    </div>
  <% end %>
</section>
